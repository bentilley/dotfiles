# haskell file snippets

# these functions are imported from the /pythonx dir in /.vim
# global !p
# from snippets.haskell import note
# endglobal

snippet ty "type expression" b
${1:expression} :: ${0:typedef}
endsnippet

snippet tc "type class constraint" w
${1:typeclass} ${2:a} => ${0:types}
endsnippet

snippet td "type definition" w
${1:type1} -> ${0:type2}
endsnippet
