# default settings
defaults
tls on
logfile ~/.msmtp.log

# accesso account
account ben.tilley@accesso.com
host smtp.office365.com
port 587
protocol smtp
auth on
from ben.tilley@accesso.com
user ben.tilley@accesso.com
#tls_certcheck off
tls_starttls on
tls_trust_file /etc/ssl/cert.pem
passwordeval python -c "import keyring; print(keyring.get_password('outlook.office365.com', 'ben.tilley@accesso.com'))"

account default : ben.tilley@accesso.com
